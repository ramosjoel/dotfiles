{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "github": {
      "headers": {
        "Authorization": "Bearer ${NPM_TOKEN_GH_GOODRX_CI}"
      },
      "url": "https://api.githubcopilot.com/mcp"
    },
    "playwright": {
      "args": [
        "@playwright/mcp@latest"
      ],
      "command": "npx"
    }
  }
}
